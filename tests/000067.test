type Element<T> is {Node<T>[] children}
type Node<T> is Element<T>|int[]

function count<T>(Node<T> n) -> int:
    if n is int[]:
        return 0
    else:
        int r = 1
        for i in 0..|n.children|:
            r = r + count(n.children[i])
        return r

